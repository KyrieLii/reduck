import HtmlWebpackPlugin, { HtmlTagObject } from 'html-webpack-plugin';
import { Compiler, Compilation } from 'webpack';
export declare class InlineChunkHtmlPlugin {
    htmlWebpackPlugin: typeof HtmlWebpackPlugin;
    tests: RegExp[];
    constructor(htmlWebpackPlugin: typeof HtmlWebpackPlugin, tests: RegExp[]);
    getInlinedScriptTag(publicPath: string, assets: Compilation['assets'], tag: HtmlTagObject): HtmlWebpackPlugin.HtmlTagObject | {
        tagName: string;
        innerHTML: string | Buffer;
        closeTag: boolean;
    };
    getInlinedCSSTag(publicPath: string, assets: Compilation['assets'], tag: HtmlTagObject): HtmlWebpackPlugin.HtmlTagObject | {
        tagName: string;
        innerHTML: string | Buffer;
        closeTag: boolean;
    };
    getInlinedTag(publicPath: string, assets: Compilation['assets'], tag: HtmlTagObject): HtmlWebpackPlugin.HtmlTagObject;
    apply(compiler: Compiler): void;
}
