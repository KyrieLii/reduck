  var store = createStore();
  test('select state should works', function () {
    var _store$use = store.use(count1Model, count2Model, function (state1, state2) {
      return {
        one: state1.value,
        two: state2.value
      };
    }),
        _store$use2 = _slicedToArray(_store$use, 1),
        state = _store$use2[0];

    expect(state).toEqual({
      one: 1,
      two: 10
    });
  });
  test('select actions should works', function () {
    var use = function use() {
      return store.use(count1Model, count2Model, function (state1, state2) {
        return {
          one: state1.value,
          two: state2.value
        };
      }, function (actions1, actions2) {
        return {
          oneAdd: actions1.add,
          twoAdd: actions2.add1
        };
      });
    };

    var _use = use(),
        _use2 = _slicedToArray(_use, 2),
        state = _use2[0],
        actions = _use2[1];

    expect(state).toEqual({
      one: 1,
      two: 10
    });
    actions.oneAdd();
    expect(use()[0]).toEqual({
      one: 2,
      two: 10
    });
    actions.twoAdd();
    expect(use()[0]).toEqual({
      one: 2,
      two: 11
    });
  });
});