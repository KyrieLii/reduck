describe('test subscribe', function () {
  test('subsribe should works for simple model', function () {
    var store = createStore();

    var _store$use = store.use(count1Model),
        _store$use2 = _slicedToArray(_store$use, 3),
        actions = _store$use2[1],
        subscribe = _store$use2[2];

    var fn = jest.fn();
    subscribe(function () {
      fn();
    });
    actions.add();
    expect(fn).toBeCalledTimes(1);
  });
  test('subscribe should works for multiple model', function () {
    var store = createStore();

    var _store$use3 = store.use([count1Model, count2Model]),
        _store$use4 = _slicedToArray(_store$use3, 3),
        actions = _store$use4[1],
        subscribe = _store$use4[2];

    var fn = jest.fn();
    subscribe(function () {
      fn();
    });
    actions.add();
    expect(fn).toBeCalledTimes(1);
    expect(store.getState()).toEqual({
      count1: {
        value: 2
      },
      count2: {
        value: 1
      }
    });
    actions.add1();
    expect(fn).toBeCalledTimes(2);
    expect(store.getState()).toEqual({
      count1: {
        value: 2
      },
      count2: {
        value: 2
      }
    });
  });
  test('unsubsribe should works', function () {
    var store = createStore();

    var _store$use5 = store.use(count1Model),
        _store$use6 = _slicedToArray(_store$use5, 3),
        actions = _store$use6[1],
        subscribe = _store$use6[2];

    var fn = jest.fn();
    var unsubsribe = subscribe(function () {
      fn();
    });
    actions.add();
    expect(fn).toBeCalledTimes(1);
    unsubsribe();
    expect(fn).toBeCalledTimes(1);
  });
});