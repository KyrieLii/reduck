describe('test useModel', function () {
  test("use model in model's action should work", function () {
    var store = createStore();

    var _store$use = store.use(count2Model),
        _store$use2 = _slicedToArray(_store$use, 2),
        actions = _store$use2[1];

    var _store$use3 = store.use(count1Model),
        _store$use4 = _slicedToArray(_store$use3, 2),
        count1Actions = _store$use4[1];

    expect(store.getState()).toEqual({
      count1: {
        value: 1
      },
      count2: {
        value: 1
      }
    });
    actions.addCount1Value();
    expect(store.getState()).toEqual({
      count1: {
        value: 1
      },
      count2: {
        value: 2
      }
    });
    count1Actions.add();
    expect(store.getState()).toEqual({
      count1: {
        value: 2
      },
      count2: {
        value: 2
      }
    });
    actions.addCount1Value();
    expect(store.getState()).toEqual({
      count1: {
        value: 2
      },
      count2: {
        value: 4
      }
    });
  });
  test('use models with same name should get a warning', function () {
    var spy = jest.spyOn(console, 'info');
    var store = createStore();
    var countModel = model('count1').define({
      state: 1
    });
    store.use(count1Model, countModel);
    expect(spy).toHaveBeenLastCalledWith('model named count1 has already mounted, so skip');
    spy.mockClear();
  });
  test('use self in model will get error', function () {
    var test = model('name').define(function (_, _ref2) {
      var use = _ref2.use;
      use(test);
      return {
        state: 1
      };
    });
    expect(function () {
      return createStore().use(test);
    }).toThrow(Error);
  });
  test('use mltiple model with primitive state get error', function () {
    var test = model('name').define({
      state: 1
    });
    expect(function () {
      return createStore().use(test, count1Model);
    }).toThrow(Error);
  });
});