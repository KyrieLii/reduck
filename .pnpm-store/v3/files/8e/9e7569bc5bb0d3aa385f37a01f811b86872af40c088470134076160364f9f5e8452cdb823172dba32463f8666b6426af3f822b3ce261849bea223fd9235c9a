import { createStore, model } from '..';
var count1Model = model('count1').define(function () {
  return {
    state: {
      value: 1
    },
    actions: {
      add: function add(state) {
        return _objectSpread(_objectSpread({}, state), {}, {
          value: state.value + 1
        });
      }
    }
  };
});
var count2Model = model('count2').define(function (_, _ref) {
  var use = _ref.use;
  use(count1Model);
  return {
    state: {
      value: 1
    },
    actions: {
      addCount1Value: function addCount1Value(state) {
        var _use = use(count1Model),
            _use2 = _slicedToArray(_use, 1),
